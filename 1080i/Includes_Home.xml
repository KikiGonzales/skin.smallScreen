        <texturefocus>home/half-tile.png</texturefocus>
        <texturenofocus>home/half-tile.png</texturenofocus>
    </include>
    <include name="Home-Menu-Button-BG">
        <control type="button">
            <include>Home-Menu-Button</include>
            <colordiffuse>ff000000</colordiffuse>
        </control>
    </include>
    <include name="Home-Menu-Visibility">
        <visible allowhiddenfocus="true">ControlGroup(300).HasFocus() | Window.IsVisible(home) | Control.HasFocus(351) | Control.HasFocus(352) | Control.HasFocus(353) | Control.HasFocus(350)</visible>
    </include>
